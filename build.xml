<project>

    <target name="clean">
        <delete dir="build"/>
    </target>

    <target name="compile">
        <mkdir dir="build/classes"/>
        <javac srcdir="httpservice/src" destdir="build/classes">
            <classpath>
                <pathelement location="lib/netty-4.1.6.Final/jar/all-in-one/netty-all-4.1.6.Final.jar"/>
            </classpath>
        </javac>
        <!--<move file="build/classes/RequestHandler.class" todir="build/classes/u/http/"/>-->
    </target>

    <target name="run">
        <java classname="u.http.HttpMain">
            <classpath>
                <pathelement location="lib/netty-4.1.6.Final/jar/all-in-one/netty-all-4.1.6.Final.jar"/>
                <pathelement location="build/classes/"/>
            </classpath>
        </java>
    </target>

</project>